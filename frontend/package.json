    "sharp": "^0.34.5",
    "svelte": "^5.0.0",
    "svelte-check": "^3.0.0",
    "tailwindcss": "^3.4.10",
    "tslib": "^2.4.0",
    "typescript": "^5.0.0",
    "vite": "^5.0.0",
    "vite-plugin-pwa": "^0.20.0",
    "vitest": "^4.0.16"
