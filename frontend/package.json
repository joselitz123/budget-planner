    "@typescript-eslint/parser": "^7.0.0",
    "autoprefixer": "^10.4.20",
    "eslint": "^8.0.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-svelte": "^2.0.0",
    "postcss": "^8.4.41",
    "prettier": "^3.0.0",
    "prettier-plugin-svelte": "^3.0.0",
